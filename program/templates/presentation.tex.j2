\documentclass[aspectratio=169]{beamer}
\usefonttheme{structurebold}

\definecolor{primary_color}{HTML}{001524}
\definecolor{primary_color_inverse}{HTML}{ffeadb}
\definecolor{secondary_color}{HTML}{2b4162}
\definecolor{secondary_color_inverse}{HTML}{d4be9d}
\definecolor{accent_color}{HTML}{fa9f42}
\definecolor{notes_color}{HTML}{72a276} %38aecc
