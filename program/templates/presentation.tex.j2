\mode<presentation> {
\usetheme{Madrid}

\setbeamercolor*{palette primary}{bg = primary_color, fg = primary_color_inverse}
\setbeamercolor*{palette secondary}{bg = secondary_color, fg = primary_color_inverse}
\setbeamercolor*{palette tertiary}{bg = primary_color, fg = primary_color_inverse}
\setbeamercolor{section in toc}{fg=primary_color,bg=white}
\setbeamercolor{normal text}{fg=primary_color}
\setbeamercolor{section number projected}{bg=accent_color,fg=primary_color}
\setbeamercolor{subsection number projected}{bg=primary_color,fg=primary_color}
\setbeamercolor{itemize item}{fg=primary_color}
\setbeamercolor{caption name}{fg=primary_color}
\setbeamertemplate{itemize item}[square]
\setbeamertemplate{itemize subitem}[circle]
\setbeamertemplate{subsection number projected}[circle]
\setbeamertemplate{bibliography item}{\insertbiblabel}
\setbeamertemplate{caption}[numbered]
\setbeamercovered{transparent}
}

\setbeamerfont{frametitle}{size=\large}

\setbeamerfont{block title}{size=\small}
